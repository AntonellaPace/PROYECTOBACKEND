<body>
    <div>
        <h1>Listado de Productos: </h1>
        {{#each products }}
        <ul>
            <li>{{this.title}}</li>
            <li>{{this.description}}</li>
            <li>Precio: ${{this.price}}</li>
            <li>Stock: {{this.stock}}</li>
        </ul>
        {{/each}}
    </div>
</body>